{
  "source": {
    "include": ["src", "README.md", "package.json"]
  },
  "opts": {
    "encoding": "utf8",
    "readme": "./README.md",
    "destination": "docs/",
    "recurse": true,
    "verbose": true,
    "template": "./node_modules/clean-jsdoc-theme",
    "theme_opts": {
      "theme": "dark"
    }
  }
}
